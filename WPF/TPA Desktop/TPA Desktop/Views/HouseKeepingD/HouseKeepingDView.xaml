<Window x:Class="TPA_Desktop.Views.HouseKeepingD.HouseKeepingDView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TPA_Desktop.Views"
        mc:Ignorable="d"
        Title="House Keeping Division" Height="300" Width="525"
        WindowStartupLocation="CenterScreen"
        Background="LightBlue">
    <Border Padding="10">
        <StackPanel>
            <Frame x:Name="Biodata"></Frame>
            <Button x:Name="addCleaningLogBtn" Margin="5" Click="addCleaningLogBtn_Click">Add Cleaning Log</Button>
            <Button x:Name="viewReservationBtn" Margin="5" Click="viewReservationBtn_Click">View All Reservation</Button>
        </StackPanel>
    </Border>
</Window>
